{
  "version": 2,
  "builds": [
    {
      "src": "api/webfile.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/templates/index.html"
    },
    {
      "src": "/video_feed",
      "dest": "/api/webfile.py"
    },
    {
      "src": "/api",
      "dest": "/api/webfile.py"
    },
    {
      "src": "/api/another",
      "dest": "/api/webfile.py"
    }
  ]
}
